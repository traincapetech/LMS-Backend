# Database Configuration
MONGO_URI=your_mongodb_atlas_connection_string

# Server Configuration
PORT=5001
JWT_SECRET=your_jwt_secret_key_here

# Admin Configuration
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin_password

# Email Configuration (Hostinger)
# Hostinger SMTP Settings
SMTP_HOST=smtp.hostinger.com
SMTP_PORT=587
EMAIL_USER=your_email@yourdomain.com
EMAIL_PASSWORD=your_email_password

# Alternative: Gmail OAuth2 (uncomment if using Gmail)
# GMAIL_CLIENT_ID=your_google_oauth_client_id
# GMAIL_CLIENT_SECRET=your_google_oauth_client_secret
# GMAIL_REFRESH_TOKEN=your_google_oauth_refresh_token
# GMAIL_ACCESS_TOKEN=your_google_oauth_access_token

# R2 Cloud Storage Configuration (Cloudflare R2)
R2_ENDPOINT=https://your-account-id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_VIDEOS=your-videos-bucket-name
R2_BUCKET_DOCS=your-docs-bucket-name
R2_BUCKET_IMAGES=your-images-bucket-name
R2_PUBLIC_URL_VIDEOS=https://your-videos-domain.com
R2_PUBLIC_URL_DOCS=https://your-docs-domain.com
R2_PUBLIC_URL_IMAGES=https://your-images-domain.com 